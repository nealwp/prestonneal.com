{
  "name": "prestonneal.com",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:build": "docker build --tag nealwp/prestonneal.com:latest --tag nealwp/prestonneal.com:$(git rev-parse --short HEAD) .",
    "docker:publish": "npm run docker:build && docker push nealwp/prestonneal.com",
    "docker:run": "docker run -d -p 8080:80 --name nealwp/prestonneal.com nealwp/prestonneal.com:latest",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
